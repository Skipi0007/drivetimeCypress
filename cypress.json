{
    "baseUrl": "https://www.drivetime.com/",
    "viewportHeight": 1080,
    "viewportWidth": 1920,
    "reporter": "mochawesome",
    "reporterOptions": {
        "reportDir": "cypress/results/specs",
        "overwrite": false,
        "html": true,
        "json": true,
        "inline": true
      },
    "scteenShots": false,
    "screenshotOnRunFailure": false, 
    "videoCompression": false,
    "screenshotConfig": {
        "blackout": [],
        "clip": null,
        "disableTimersAndAnimations": true,
        "log": true,
        "scale": false,
        "timeout": 30000
    },
    "videoUploadOnPasses": false
    
}
