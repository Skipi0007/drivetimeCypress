{
    "baseUrl": "https://www.drivetime.com/",
    "viewportHeight": 1080,
    "viewportWidth": 1920,
    "reporter": "mochawesome",
    "reporterOptions": {
        "reportFilename": "myReporter",
        "quiet": true
      },
    "env": {
        "screenShots": true
    },
    "screenShots": true,
    "screenshotConfig": {
        "blackout": [],
        "clip": null,
        "disableTimersAndAnimations": true,
        "log": true,
        "scale": false,
        "timeout": 30000
    },
    "videoCompression": false,
    "videoUploadOnPasses": false
    
}